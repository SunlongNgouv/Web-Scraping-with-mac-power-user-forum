        "##DATA PREPROCESSING"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "vVB8c7ZObnvU"
      },
      "source": [
        "# clean data\n",
        "\n",
        "def pre_process(text):\n",
        "    # remove all leading and trailing spaces from the strings\n",
        "    text = text.strip()\n",
        "    \n",
        "    # lowercase\n",
        "    text=text.lower()\n",
        "    \n",
        "    # remove tags\n",
        "    text=re.sub(\"\",\"\",text)\n",
        "    \n",
        "    # remove special characters and digits\n",
        "    text=re.sub(\"(\\\\d|\\\\W)+\",\" \",text)\n",
        "\n",
        "    # stem words\n",
        "    words=re.split(\"\\\\s+\",text)\n",
        "    porter_stemmer = PorterStemmer()\n",
        "    stemmed_words=[porter_stemmer.stem(word=word) for word in words]\n",
        "    #return ' '.join(stemmed_words)\n",
        "\n",
        "    wordnet_lemmatizer = WordNetLemmatizer()\n",
        "    lemmatized_words = [wordnet_lemmatizer.lemmatize(word=word) for word in stemmed_words] \n",
        "    return ' '.join(lemmatized_words)\n",
        "\n",
        "# combine two columns, Topic and Comment, as Text\n",
        "df['text'] = df['TOPIC'] + df['COMMENT']\n",
        "df['text'] = df['text'].apply(lambda x:pre_process(x))\n"
      ],
      "execution_count": null,
      "outputs": []
